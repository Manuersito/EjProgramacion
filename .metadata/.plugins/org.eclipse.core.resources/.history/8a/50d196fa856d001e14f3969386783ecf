package Tema_3_Aplicacion;

import java.util.Iterator;

public class App_6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int lado = 4;
		
		for (int i = 0; i <= lado; i++) {
			for (int j = 0; j < lado-i-1; j++) {
				System.out.print("* ");
							}
			
			System.out.println("");
		}
	}

}
