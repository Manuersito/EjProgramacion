package Tema_3_Aplicacion;

import java.util.Iterator;

public class App_6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int lado = 4;
		
		for (int i = 0; i < lado; i++) {
			for (int j = lado; j < lado; j--) {
				System.out.println(" ");
				for (int j2 = 0; j2 < lado; j2--) {
				System.out.println("*");	
				}
			}
		}
	}

}
