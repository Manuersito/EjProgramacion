package Tema_3_Ampliacion;

public class Amp_2 {
	public static void main(String[] args) {
		int num,max=0,min=0,media;
		for (int i = 0; i < 50; i++) {
			num = (int) (Math.random()*99+100);
			System.out.println(num);
			
			if (num>max) {
				max=num;
				System.out.println("el maximo es: "+max);
			} else {
				max=max;
				System.out.println("el maximo es: "+max);
			}
		}
	}
}
